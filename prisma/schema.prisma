// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Products {
  lm            Int     @unique
  name          String
  free_shipping Boolean
  description   String
  price         Float
  category      Int
}
